    "isError",
    "isDate",
    "isArguments",
    "isFunction",
    "isRegExp",
    "isArray",
    "isNumber",
    "isString",
    "isBoolean",
    "isNull",
    "isUndefined",
    "isMap",
    "isWeakMap"
